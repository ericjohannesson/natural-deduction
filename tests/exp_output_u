examples/itm0
T(x,y) := x < y

I(z) := 0 < z ∧ ∀y (y < z → y' < z)
examples/itm1
Prf_1 :=

                  ∀x ∀y x + y' = (x + y)'      
                  -----------------------∀E    
∀x x + 0 = x      ∀y 0' + y' = (0' + y)'       
------------∀E    -------------------------∀E  
0' + 0 = 0'          0' + 0' = (0' + 0)'       
---------------------------------------------=E
                0' + 0' = 0''                  

# Prf_1 is VALID.
# PROVES ∀x x + 0 = x, ∀x ∀y x + y' = (x + y)' ⊢ 0' + 0' = 0''.

examples/itm10
Sub :=

∀x (P(x) → Q(x))              
----------------∀E            
  P(c) → Q(c)         P(c)    
--------------------------→E  
           Q(c)               
----------------------------∃I
          ∃x Q(x)             

# Sub is VALID.
# PROVES ∀x (P(x) → Q(x)), P(c) ⊢ ∃x Q(x).


Main :=

∀x (P(x) → Q(x))                            
----------------∀E                          
  P(c) → Q(c)         P(c)                  
--------------------------→E                
           Q(c)                             
----------------------------∃I              
          ∃x Q(x)                 ∃x P(x)   
-----------------------------------------∃E0
                 ∃x Q(x)                    

# Main is VALID.
# PROVES ∀x (P(x) → Q(x)), P(c), ∃x P(x) ⊢ ∃x Q(x).

examples/itm11
